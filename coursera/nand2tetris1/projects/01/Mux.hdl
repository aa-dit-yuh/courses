/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in = sel, out = notSEL);
    Nand(a = a, b = notSEL, out = AnandNotSEL);
    Nand(a = sel, b = b, out = BnandSEL);
    Nand(a = AnandNotSEL, b = BnandSEL, out = out);
}